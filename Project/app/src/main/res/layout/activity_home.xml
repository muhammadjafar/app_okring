<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jafarabdi.project.beranda.Home">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textcatering"
        android:layout_marginLeft="10dp"
        android:textSize="16dp"
        android:gravity="center"
        android:textColor="@color/colorPrim"
        android:text="Daftar Catering"/>
    <ListView
        android:layout_below="@+id/textcatering"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/list"></ListView>

</RelativeLayout>
